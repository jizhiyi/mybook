{
    "title": "jizhihong's notebook",
    "author": "jizhihong",
    "description": "Living in a dream",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "back-to-top-button",
        "chapter-fold",
        "code",
        "splitter",
        "-lunr", 
        "-search", 
        "search-pro",
        "-insert-logo",
        "custom-favicon",
        "-sharing",
        "sharing-plus",
        "todo",
        "tbfed-pagefooter",
        "advanced-emoji",
        "anchor-navigation-ex",
        "katex",
        "popup"
    ],
    "pluginsConfig": {
        "favicon": "icon/favicon.ico",
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": ["qq"]
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy jizhihong 2020",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
